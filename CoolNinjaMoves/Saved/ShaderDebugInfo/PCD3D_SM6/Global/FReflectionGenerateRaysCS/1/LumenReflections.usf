	PointInRect.y = dot( Rect.Axis[1], PointOnPlane - Rect.Origin );

	
    float2 RectUV = (PointInRect + Rect.Offset) / max(0.0001f, Rect.FullExtent) * float2(0.5, -0.5) + 0.5;
	
